{
    "name": "romegadigital/testsuite",
    "description": "Common test methods utilized in Romega packages.",
    "keywords": [
        "romega",
        "testing"
    ],
    "homepage": "https://romegasoftware.com/",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Braden Keith",
            "email": "bkeith@romegasoftware.com",
            "homepage": "https://bradenkeith.com/",
            "role": "Developer"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "illuminate/support": "~5.8|~6.0|~7.0",
        "spatie/laravel-permission": "^3.0",
        "codedungeon/phpunit-result-printer": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "RomegaDigital\\TestSuite\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RomegaDigital\\TestSuite\\Test\\": "test"
        }
    },
    "minimum-stability": "dev" 
}
